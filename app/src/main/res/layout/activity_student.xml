<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:context=".student">


    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20sp"
        android:background="#ffffff"
        android:src="@drawable/menu" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10sp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/numbertxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="30sp"
            android:textColor="#000000"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="님 어서오세요."
            android:textSize="15sp"
            />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20sp"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/timetablebtn"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="10sp"
                android:adjustViewBounds="true"
                android:background="#ffffff"
                android:onClick="onClick"
                android:scaleType="fitCenter"
                android:src="@drawable/timetablebtn" />

            <TextView
                android:layout_width="100sp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:gravity="center"
                android:text="강의시간표" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/gradebtn"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="10sp"
                android:adjustViewBounds="true"
                android:background="#ffffff"
                android:onClick="onClick"
                android:scaleType="fitCenter"
                android:src="@drawable/gradebtn" />

            <TextView
                android:layout_width="100sp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:gravity="center"
                android:text="성적조회" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/profilebtn"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="10sp"
                android:adjustViewBounds="true"
                android:background="#ffffff"
                android:onClick="onClick"
                android:scaleType="fitCenter"
                android:src="@drawable/profilebtn" />

            <TextView
                android:layout_width="100sp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:gravity="center"
                android:text="학생정보" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/applybtn"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="10sp"
                android:adjustViewBounds="true"
                android:background="#ffffff"
                android:onClick="onClick"
                android:scaleType="fitCenter"
                android:src="@drawable/applybtn" />

            <TextView
                android:layout_width="100sp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:gravity="center"
                android:text="수강신청" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/scholarbtn"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="10sp"
                android:adjustViewBounds="true"
                android:background="#ffffff"
                android:onClick="onClick"
                android:scaleType="fitCenter"
                android:src="@drawable/scholarbtn" />

            <TextView
                android:layout_width="100sp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:gravity="center"
                android:text="장학조회" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/evaluationbtn"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="10sp"
                android:adjustViewBounds="true"
                android:background="#ffffff"
                android:onClick="onClick"
                android:scaleType="fitCenter"
                android:src="@drawable/evaluationbtn" />

            <TextView
                android:layout_width="100sp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:gravity="center"
                android:text="강의평가" />
        </LinearLayout>
    </LinearLayout>

